---->>>>>>>>>>>Funções e Estrutura de repetição<<<<<<<<<<<----
	

//Criar um programa que calcule a média 
//das turmas de alunos e envie 
//uma mensagem do calculo da mpedia


const alunosDaTurmaA = [
{
  nome: "Victor",
  nota: 1.8
},  
{
  nome: "Diego",
  nota: 10
},
{
  nome: "Marcos",
  nota: 2
}    
]

const alunosDaTurmaB = [
{
   nome: "Carlos",
   nota: 10
},  
{
  nome: "Pedro",
  nota: 10
},
{
  nome: "Ziro",
  nota: 0
}
]

function CalculoMedia(alunos) {
  return (alunos[0].nota + alunos[1].nota + alunos[2].nota) / 3
}

const media1 = CalculoMedia(alunosDaTurmaA)
const media2 = CalculoMedia(alunosDaTurmaB)



function enviarMensagem(media, turma){
if (media > 5 ) {
  console.log(`A media da turma ${turma} foi de ${media}. Parabéns`)
} else {
  console.log(`A média da turma ${turma} é menor que 5`)
}
}

enviarMensagem(media1, 'turmaA')
enviarMensagem(media2, 'TurmaB')  

//const media = (alunos[0].nota + alunos[1].nota + alunos[2].nota) / 3


