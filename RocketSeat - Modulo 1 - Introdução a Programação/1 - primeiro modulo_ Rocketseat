---->>>>>>>>>>>DESAFIO 1 INTRODUÇÃO À PROGRAMAÇÃO<<<<<<<<<<<----
// Calculo de IMC

const nome = "Victor"
const peso = 93
const altura = 1.73
const sexo = "Masculico"

const IMC = (peso / (altura * altura))
console.log(IMC)

//Exibindo mensagem
//Basta apenas alterar o valor de Peso para mudar mensagem.

if (IMC >= 30) {
    console.log (`Carlos você está acima do peso`)
} else if (IMC <= 29.99) {
    console.log (`Carlos você não acima do peso`)
}

