---->>>>>>>>>>>Estrutura de repetição<<<<<<<<<<<----



//Criar um programa que calcule a média 
//das turmas de alunos e envie 
//uma mensagem do calculo da mpedia


const alunosDaTurmaA = [
{
  nome: "Victor",
  nota: 1.8
},  
{
  nome: "Diego",
  nota: 10
},
{
  nome: "Marcos",
  nota: 2
},    
{
  nome: "dado",
  nota: 10
}   
]

const alunosDaTurmaB = [
{
   nome: "Carlos",
   nota: 10
},  
{
  nome: "Pedro",
  nota: 10
},
{
  nome: "Ziro",
  nota: 10
},
{
  nome: "Novato",
  nota: 1
}
]


function CalculoMedia(alunos) {
  //for (let /*permite que mude o valor de*/ i = 0; i < alunos.length; i++) {
    // console.log(i);
let soma = 0
for (let i = 0; i < alunos.length; i++) { 
  soma = soma + alunos[i].nota  
}
  }


const media1 = CalculoMedia(alunosDaTurmaA)
const media2 = CalculoMedia(alunosDaTurmaB)



function enviarMensagem(media, turma){
if (media > 5 ) {
  console.log(`A media da turma ${turma} foi de ${media}. Parabéns`)
} else {
  console.log(`A média da turma ${turma} é menor que 5`)
}
}

enviarMensagem(media1, 'turmaA')
enviarMensagem(media2, 'TurmaB')  

//const media = (alunos[0].nota + alunos[1].nota + alunos[2].nota) / 3

-------------------------------------------------------------------------------------------------------------------------
---->>>>>>>>>>>Booleans True e False<<<<<<<<<<<----

const alunosDaTurmaA = [
  {
    nome: "Victor",
    nota: 9.8
  },
  {
    nome: "Diego",
    nota: 10
  },
  {
    nome: "Marcos",
    nota: 2
  },
  {
    nome: "dado",
    nota: 10
  }
]

const alunosDaTurmaB = [
  {
    nome: "Carlos",
    nota: 10
  },
  {
    nome: "Pedro",
    nota: 10
  },
  {
    nome: "Ziro",
    nota: 10
  },
  {
    nome: "Novato",
    nota: 1
  }
]


function CalculoMedia(alunos) {
  //for (let /*permite que mude o valor de*/ i = 0; i < alunos.length; i++) {
  // console.log(i);
  let soma = 0
  for (let i = 0; i < alunos.length; i++) {
    soma = soma + alunos[i].nota
  }
  
  const media = soma / alunos.length

  return media
}


const media1 = CalculoMedia(alunosDaTurmaA)
const media2 = CalculoMedia(alunosDaTurmaB)



function enviarMensagem(media, turma) {
  if (media > 5) {
    console.log(`A media da turma ${turma} foi de ${media}. Parabéns`)
  } else {
    console.log(`A média da turma ${turma} é menor que 5`)
  }
}

enviarMensagem(media1, 'turmaA')
enviarMensagem(media2, 'TurmaB')


function marcarComoReprovado(aluno) {
  aluno.reprovado = false
  if (aluno.nota < 5) {
    aluno.reprovado = true
  }
}


function enviarMensagemReprovado(aluno) {
  if (aluno.reprovado) {
    console.log(` O aluno ${aluno.nome} está reprovado!`)
  }
}


function alunosReprovado(alunos) {
  for (let aluno of alunos) {
    marcarComoReprovado(aluno)
    enviarMensagemReprovado(aluno)
  }
}

alunosReprovado(alunosDaTurmaA)
alunosReprovado(alunosDaTurmaB)