---->>>>>>>>>>>DESAFIO 2 INTRODUÇÃO À PROGRAMAÇÃO<<<<<<<<<<<----
// Calculo de Aposentadoria

const nome1 = "Miriam"
const sexo1 = "Feminino"
const idade1 = 78
const contribuicao1 = 23

const nome2 = "Marcos"
const sexo2 = "Masculino"
const idade2 = 56
const contribuicao2 = 26

const regra1 = (idade1) + (contribuicao1)
const regra2 = (idade2) + (contribuicao2)

//O tempo de contribuição mínimo para homens é de 35 anos e, para mulheres, 30 anos;
//Utilizando a regra 85-95, a soma da idade com o tempo de contribuição do homem precisa ser de no mínimo 95 anos, enquanto a mulher precisa ter no mínimo 85 anos na soma;
//console.log (regra1 , regra2)

if (regra1 >= 85 ) {
    console.log ('Silvana, você pode já pode se aponsetar')
} else {
    console.log (`Silvana, você ainda não pode se aposentar`)
}
if (regra2 >=90) {
    console.log ('Marcos/, você pode já pode se aponsetar')
} else {
    console.log (`Marcos, você ainda não pode se aposentar`)
}
    
    
	
	
