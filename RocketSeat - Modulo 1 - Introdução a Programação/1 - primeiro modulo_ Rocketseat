---->>>>>>>>>>>Condições<<<<<<<<<<<----

// Criar um programa que calcula a media
// das notas entre os alunos e envia
// Mensagem do calculo da media.
// Se a media for > 5, parabenizar a tuma



// Criar uma variavel string = criar um texto
// As aspas pode ser simples ' ' ou duplo "" ""

const aluno01 = "Victor"
const notaAluno01 = 9.8
const aluno02 = 'Marcos'
const notaAluno02 = 10
const aluno03 = 'Fernanda'
const notaAluno03 = 2



const media = (notaAluno01 + notaAluno02 + notaAluno03) / 3
//const nome3 = `Antonio e ${nome}` //consigo colocar uma nova variavel com a ` `

// Condições

if (media > 5) {
    console.log(`A média for de ${media}. Parabéns`)
} else {
    console.log(`A média é menor que 5`)
}
//console.log(media < 5)

